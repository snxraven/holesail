{
  "name": "holesail",
  "version": "1.10.1",
  "description": "Holesail is TCP/UDP peer-to-peer proxy. Holesail let's you instantly share any application running on a specific port from your local computer.",
  "main": "holesail.mjs",
  "pear": {
    "name": "holesail",
    "type": "terminal",
    "stage": {
      "ignore": [
        ".github",
        ".gitignore",
        ".idea",
        ".vscode"
      ]
    }
  },
  "imports": {
    "util": {
      "bare": "bare-utils",
      "default": "util"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    }
  },
  "scripts": {
    "test": "standard && node test.mjs",
    "refresh": "rm package-lock.json && rm -rf node_modules && npm i && npm update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holesail/holesail.git"
  },
  "keywords": [
    "holesail",
    "hyperdht",
    "holepunch"
  ],
  "author": "supersuryaansh",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/holesail/holesail/issues"
  },
  "homepage": "https://github.com/holesail/holesail#readme",
  "pkg": {
    "assets": [
      "includes/**/*"
    ]
  },
  "dependencies": {
    "b4a": "^1.6.6",
    "bare-crypto": "^1.4.0",
    "bare-events": "^2.5.0",
    "bare-fs": "^4.0.1",
    "bare-http1": "^4.0.2",
    "bare-net": "^2.0.0",
    "bare-os": "^3.3.0",
    "bare-path": "^3.0.0",
    "bare-process": "^4.0.0",
    "bare-querystring": "^1.0.0",
    "bare-subprocess": "^5.0.2",
    "bare-tty": "^5.0.1",
    "bare-utils": "1.1.1",
    "barely-colours": "^1.0.0",
    "barely-pm2": "^0.0.4",
    "barely-sha256": "^0.0.1",
    "brittle": "^3.10.0",
    "child_process": "npm:bare-subprocess@^5.0.2",
    "cli-box": "^6.0.10",
    "crypto": "npm:bare-crypto@^1.3.1",
    "events": "npm:bare-events@^2.5.0",
    "fs": "npm:bare-node-fs@^1.0.2",
    "graceful-goodbye": "^1.3.0",
    "holesail-client": "^2.0.1",
    "holesail-server": "^2.1.1",
    "http": "npm:bare-http1@^4.0.2",
    "hyper-cmd-lib-keys": "^0.1.1",
    "hyperdht": "^6.20.0",
    "livefiles": "^1.0.0",
    "minimist": "^1.2.8",
    "module": "npm:bare-module@^4.8.3",
    "net": "npm:bare-net@^2.0.0",
    "os": "npm:bare-os@^3.3.0",
    "paparam": "^1.8.0",
    "path": "npm:bare-path@^3.0.0",
    "process": "npm:bare-process@^4.2.0",
    "qrcode-terminal": "^0.12.0",
    "querystring": "npm:bare-querystring@^1.0.0",
    "ready-resource": "^1.1.1",
    "tty": "npm:bare-tty@^5.0.1",
    "util": "npm:bare-utils@^1.1.1",
    "which-runtime": "^1.2.1",
    "z32": "^1.1.0"
  },
  "bin": {
    "holesail": "./holesail.js"
  },
  "devDependencies": {
    "standard": "^17.1.2"
  }
}
